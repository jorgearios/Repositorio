{
  "name": "divi5-amelia/catalog",
  "d4Shortcode": "divi_catalog",
  "title": "Amelia - Catalog (Legacy)",
  "titles": "Amelia Catalogs (Legacy)",
  "category": null,
  "moduleClassName": "divi5_amelia_catalog",
  "moduleOrderClassName": "divi5_amelia_catalog",
  "moduleIcon": "amelia/step-booking",
  "attributes": {
    "module": {
      "type": "object",
      "selector": "{{selector}}",
      "default": {
        "meta": {
          "adminLabel": {
            "desktop": {
              "value": "Amelia Catalog"
            }
          }
        }
      },
      "settings": {
        "meta": {
          "adminLabel": {}
        },
        "decoration": {
          "background": {},
          "sizing": {},
          "spacing": {},
          "border": {},
          "boxShadow": {},
          "filters": {},
          "transform": {},
          "animation": {},
          "overflow": {},
          "disabledOn": {},
          "transition": {},
          "position": {},
          "zIndex": {},
          "scroll": {},
          "sticky": {}
        }
      }
    },
    "catalog_view": {
      "type": "object",
      "settings": {
        "innerContent": {
          "groupType": "group-item",
          "item": {
            "groupName": "mainContent",
            "priority": 5,
            "render": true,
            "attrName": "catalog_view.innerContent",
            "label": "Catalog View",
            "description": "Select what to display in the catalog",
            "component": {
              "name": "divi/select",
              "type": "field"
            }
          }
        }
      }
    },
    "categories_catalog": {
      "type": "object",
      "settings": {
        "innerContent": {
          "groupType": "group-item",
          "item": {
            "groupName": "mainContent",
            "priority": 10,
            "render": true,
            "attrName": "categories_catalog.innerContent",
            "label": "Select Categories",
            "description": "Select categories to display (multi-select)",
            "component": {
              "name": "divi/select",
              "type": "field"
            }
          }
        }
      }
    },
    "services_catalog": {
      "type": "object",
      "settings": {
        "innerContent": {
          "groupType": "group-item",
          "item": {
            "groupName": "mainContent",
            "priority": 15,
            "render": true,
            "attrName": "services_catalog.innerContent",
            "label": "Select Services",
            "description": "Select services to display (multi-select)",
            "component": {
              "name": "divi/select",
              "type": "field"
            }
          }
        }
      }
    },
    "packages_catalog": {
      "type": "object",
      "settings": {
        "innerContent": {
          "groupType": "group-item",
          "item": {
            "groupName": "mainContent",
            "priority": 20,
            "render": true,
            "attrName": "packages_catalog.innerContent",
            "label": "Select Packages",
            "description": "Select packages to display (multi-select)",
            "component": {
              "name": "divi/select",
              "type": "field"
            }
          }
        }
      }
    },
    "type": {
      "type": "object",
      "settings": {
        "innerContent": {
          "groupType": "group-item",
          "item": {
            "groupName": "mainContent",
            "priority": 30,
            "render": true,
            "attrName": "type.innerContent",
            "label": "Show Type",
            "description": "Filter by type",
            "component": {
              "name": "divi/select",
              "type": "field"
            }
          }
        }
      }
    },
    "trigger": {
      "type": "object",
      "settings": {
        "innerContent": {
          "groupType": "group-item",
          "item": {
            "groupName": "mainContent",
            "priority": 50,
            "render": true,
            "attrName": "trigger.innerContent",
            "label": "Trigger Element Selector",
            "description": "CSS selector for the element that will trigger the catalog",
            "component": {
              "name": "divi/text",
              "type": "field"
            }
          }
        }
      }
    }
  }
}
